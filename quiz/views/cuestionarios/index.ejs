<table border="1px" width="400" style="margin:0 auto;">
<caption><h2>Cuestionarios</h2>
	<%if(session.user) {%>
		<a href="/admin/cuestionarios/new"><button type="button">Crear cuestionario</button></a>
	<%}%>
</caption>
<tr><td>Creador</td><td>Observaciones</td><td>Fecha</td><td colspan="2">Acciones</td></tr>
<% var i; for (i = 0 ;i < cuestionarios.length ; i++){ %>
    <tr>
		<tr>
		<td width="10">
<% if(cuestionarios[i].Profesor) { %>
<%= cuestionarios[i].Profesor.nombre %>
<% } %>
</td>
        <td><%= cuestionarios[i].observaciones %></td>    
        <td><%= cuestionarios[i].fechaFin.toDateString() %></a></td>
        <%if(session.user){%>
        <td><a href="/admin/cuestionarios/<%= cuestionarios[i].id %>/edit"><button>editar</button></a></td>
        <td><a href="/admin/cuestionarios/<%= cuestionarios[i].id %>/preguntas"><button>preguntas</button></a></td>
	<td><a href="/admin/cuestionarios/<%= cuestionarios[i].id %>/grupos"><button>grupos</button></a></td>
        <td><form method="post" action="/admin/cuestionarios/<%= cuestionarios[i].id %>/duplicar?_method=post">
            <a href="/admin/cuestionarios/<%= cuestionarios[i].id %>/duplicar"><button>Duplicar</button></a>
        </form></td>
        <td><form method="post" action="/admin/cuestionarios/<%= cuestionarios[i].id %>?_method=delete">
            <button type="submit" onClick="return confirm('Borrar: <% cuestionarios[i].observaciones %>');">Borrar</button>
        </form></td>
    </tr>

	
	<% } %>
	</tr>
<% } %>
</table><br/>
